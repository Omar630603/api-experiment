{
  "name": "api-experiment",
  "version": "1.0.0",
  "description": "NodeJS and MongoDB API application",
  "main": "server.js",
  "scripts": {
    "testAA": "cross-env NODE_ENV=test jest --testTimeout=40000 --detectOpenHandles",
    "testA01": "cross-env NODE_ENV=test jest -i tests/testA01.test.js --testTimeout=20000",
    "testA01-web": "cross-env NODE_ENV=test jest -i tests/testA01.web.test.js --testTimeout=20000 --updateSnapshot",
    "testA02": "cross-env NODE_ENV=test jest -i tests/testA02.test.js --testTimeout=20000",
    "testA03": "cross-env NODE_ENV=test jest -i tests/testA03.test.js --testTimeout=20000",
    "testA04": "cross-env NODE_ENV=test jest -i tests/testA04.test.js --testTimeout=20000",
    "testA05": "cross-env NODE_ENV=test jest -i tests/testA05.test.js --testTimeout=20000",
    "start": "node server.js",
    "dev": "nodemon server.js"
  },
  "jest": {
    "setupFilesAfterEnv": [
      "jest-expect-message"
    ],
    "ci": true
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Omar630603/api-experiment.git"
  },
  "keywords": [
    "NodeJS",
    "ExpressJS",
    "API",
    "MongoDB"
  ],
  "author": "omar",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Omar630603/api-experiment/issues"
  },
  "homepage": "https://github.com/Omar630603/api-experiment#readme",
  "devDependencies": {
    "cross-env": "^7.0.3",
    "jest": "^29.4.2",
    "jest-expect-message": "^1.1.3",
    "jest-image-snapshot": "^6.1.0",
    "nodemon": "^2.0.20",
    "puppeteer": "^19.7.2",
    "supertest": "^6.3.3"
  },
  "dependencies": {
    "dotenv": "^16.0.3",
    "ejs": "^3.1.8",
    "express": "^4.18.2",
    "express-ejs-layouts": "^2.5.1",
    "mongoose": "^6.9.2",
    "mongoose-slug-generator": "^1.0.4"
  }
}
